        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#09090A"
      },
      "package": "com.jouiwnl.stepbymobile"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "cb99a005-1c42-4782-86d1-5ea9d0cb2dd0"
      }
    }
  }
